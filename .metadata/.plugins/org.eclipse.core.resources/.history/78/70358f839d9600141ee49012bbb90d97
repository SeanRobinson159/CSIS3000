import java.util.Scanner;

public class week1_1 {

	public static void main(String[] args) {
		String[] setA = new String[10];
		Scanner input = new Scanner(System.in);
		System.out.println("Please input setA (seperated by commas)");
		setA = input.nextLine().split(",");
		
		printSet(setA);
		
		int n = setA.length;
		 for(int i = 0; i < 8; i++){
			 int [] iBinary = toBinary(i);
			 for(int j = iBinary.length-1; j > -1; j--){
				 System.out.print(iBinary[j]);
			 }
			 System.out.println();
		 }
		
		
		
		input.close();
	}
	
	public static void printSet(String[] setA){
		System.out.print("You inputed the set {");
		for (int i = 0; i < setA.length; i++) {
			System.out.print(setA[i]);
			if(i < setA.length-1){
				System.out.print(",");
			}
		}
		System.out.println("}");
	}


	public static int[] toBinary(int a){
		int binary[] = new int[4];
        int index = 0;
        while(a > 0){
            binary[index++] = a%2;
            a = a/2;
        }
        return binary;

	}
}