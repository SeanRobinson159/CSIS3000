//Find the least nonnegative residue mod 13 of these integers: 22, 100, 1001, -1, -100, -1000, 220, 230.

public class Week02_1 {

	public static void main(String[] args) {
		int x[] = new int[10];
		
		x[0] = 22%13;
		x[1] = 100%13;
		x[2] = 1001%13;
		x[3] = -1%13;
		x[4] = -100%13;
		x[5] = -1000%13;
		x[6] = 220%13;
		x[7] = 230%13;
		
		for(int i =0; i <x.length; i++){
			System.out.println("x["+i+"] = "+x[i]);
		}
		
	}

}
