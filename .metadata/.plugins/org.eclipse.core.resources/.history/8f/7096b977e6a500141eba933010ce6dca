//Write a program that, given a positive integer n, prints out all the subsets of {1,2,â€¦,n}.
//Hint: (use Integer toBinaryString() and the String method toCharArray() ). 

import java.util.Scanner;

public class week1_1 {
	// public static String[] setA = new String[10];
	static int setA = 0;

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		int a = 30;
		int n = a;
		int total = (int) Math.pow(2, n);

		for (int i = 0; i < total; i++) {
			char[] binary = new char[64];
			binary = Integer.toBinaryString(a).toCharArray();

			System.out.print("{ ");
			for (int j = 0; j < n-1; j++) {
				//System.out.println(iBinary[j]);
				if (binary[j] == '1') {
					System.out.print(j + " ");
				}
			}
			System.out.println("}");
		}
		input.close();
	}
}